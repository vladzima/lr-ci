    "dayjs": "^1.10.4",
    "graphql": "^16.2.0",
    "inquirer": "^8.0.0",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "marked": "^2.0.0",
    "marked-terminal": "4.1.1",
    "ora": "5.4.1",
    "string-width": "4.2.2",
